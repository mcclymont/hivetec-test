<p id="notice"><%= notice %></p>

<h1>Imports</h1>

<%= link_to 'New Import', new_import_path %>
<br />
<br />

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imports.each do |import| %>
      <tr>
        <td><%= link_to "Import #{import.id}", import %></td>
        <td><%= import.status.humanize %></td>
      </tr>
    <% end %>
  </tbody>
</table>
